<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ViewSettingsDialog
		id="sortDialog"
		confirm="onSortListConfirm">
		<sortItems>
			<ViewSettingsItem
				id="sortClave1"
				text="{i18n>titleItem}"
				key="title"
                selected="true"/>
			<ViewSettingsItem
				id="sortClave2"
				text="{i18n>date}"
				key="date"/>
		</sortItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>